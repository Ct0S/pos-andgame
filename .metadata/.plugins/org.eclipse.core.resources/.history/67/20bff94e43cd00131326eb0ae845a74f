public class Enemy extends AnimatedSprite {
	
	public Enemy(float pX, float pY, 
			ITiledTextureRegion textureRegion, Engine engine) {
		super(pX, pY, textureRegion, engine.getVertexBufferObjectManager());
		
	}

}
